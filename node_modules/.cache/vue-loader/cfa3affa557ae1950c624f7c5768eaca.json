/js/EventHub.js'\nimport { checkWidth } from '@/assets/js/CommonMethods.js'\n\nexport default {\n  data () {\n    return {\n      collapse: checkWidth(),\n      items: [\n        {\n          icon: 'el-icon-tickets',\n          index: 'overview',\n          title: '系统概览'\n        },\n        {\n          icon: 'el-icon-sort',\n          index: 'user',\n          title: '用户管理',\n          subs: [\n            {\n              icon: 'el-icon-arrow-right',\n              index: 'users',\n              title: '普通用户'\n            },\n            {\n              icon: 'el-icon-d-arrow-right',\n              index: 'vips',\n              title: 'VIP 用户'\n            }\n          ]\n        }\n      ]\n    }\n  },\n  computed: {\n    onRoutes () {\n      return this.$route.path.replace('/', '')\n    }\n  },\n  created () {\n    // 通过 Event Bus 进行组件间通信，来折叠侧边栏\n    eventHub.$on('collapse', msg => {\n      this.collapse = msg\n    })\n  }\n}\n</script>\n\n<style scoped>\n.sidebar {\n  display: block;\n  position: absolute;\n  left: 0;\n  top: 70px;\n  bottom: 0;\n}\n\n.sidebar-el-menu:not(.el-menu--collapse) {\n  width: 200px;\n}\n\n.sidebar > ul {\n  height: 100%;\n}\n</style>\n"]}]}